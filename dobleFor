# seleccion amplia

for i to n
	señal [i] = selected ("Sound", i)
endfor

for i to n
	etiquetas [i] = selected ("TextGrid", i)
endfor

for i to n
	tono [i] = selected ("Pitch", i)
endfor

for i to n
selectObject: señal [i], etiquetas [i], tono [i]
	pitch = selected ("Pitch")
	sonido = selected ("Sound")
	textgrid = selected ("TextGrid")
	textgrid$ = selected$ ("TextGrid")
appendInfoLine: señal [i],tab$, etiquetas [i], tab$, tono [i], tab$,textgrid$
endfor
